{
  "name": "lola-handbook",
  "description": "Lola Tech's Handbook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "localcms": "netlify-cms-proxy-server"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json, md, mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "4.5.10",
    "@openlab/vercel-netlify-cms-github": "1.1.1",
    "@picocss/pico": "1.5.0",
    "next": "12.1.6",
    "next-mdx-remote": "3.0.8",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rehype-img-size": "1.0.1",
    "remark-autolink-headings": "7.0.1",
    "remark-extract-toc": "1.1.0",
    "remark-parse": "10.0.1",
    "remark-slug": "7.0.1",
    "unified": "10.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@types/mdx-js__react": "1.5.5",
    "@types/node": "17.0.14",
    "@types/react": "17.0.45",
    "@types/react-dom": "18.0.4",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "netlify-cms-proxy-server": "1.3.24",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  }
}

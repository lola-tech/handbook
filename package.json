{
  "name": "lola-handbook",
  "description": "Lola Tech's Handbook",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "localcms": "netlify-cms-proxy-server"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json, md, mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "4.5.4",
    "@openlab/vercel-netlify-cms-github": "1.1.1",
    "@picocss/pico": "1.4.4",
    "next": "12.0.10",
    "next-mdx-remote": "3.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-img-size": "1.0.1",
    "remark-autolink-headings": "7.0.1",
    "remark-extract-toc": "1.1.0",
    "remark-parse": "10.0.1",
    "remark-slug": "7.0.1",
    "unified": "10.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/mdx-js__react": "1.5.5",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.3",
    "netlify-cms-proxy-server": "1.3.23",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  }
}
